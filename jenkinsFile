@Library('shared-library@master') _
node('master'){
    ansiColor('xterm'){
        env.LOG_LEVEL = "INFO"
        println "log level is set to: " + env.LOG_LEVEL
        
        env.AppLogLevel = "ERROR" 
        println "Application log level is set to: " + env.AppLogLevel
        
        withEnv(['APP_LOG_LEVEL=' + env.AppLogLevel ]) {
            java_jenkinsfile()
        }
    }
}
